fn factorial n is
	if = 0 n
		1
	* n factorial - n 1

fn ++ a b is __strconcat a b

fn print_factorials n is
	@ if = 0 n
		null
		print_factorials - n 1
	print ++ "fact(" ++ n ++ ") = " factorial n

fn count_to n is
	if = n 0
		print "Finished!"
	count_to - n 1

